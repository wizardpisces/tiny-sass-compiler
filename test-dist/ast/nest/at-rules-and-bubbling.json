    "at-rules-and-bubbling.scss": ".component {\n    width: 300px;\n    .selector{\n        @media (min-width: 1280px) {\n            width: 800px;\n        }\n    }\n    @media (min-width: 768px) {\n        width: 600px;\n        .selector2{\n            @media (min-resolution: 192dpi) {\n                background-image: url(/img/retina2x.png);\n            }\n        }\n        .selector3{\n            color:red;\n        }\n    }\n}\n\n$layout-breakpoint-small: 960px;\n\n@media (min-width: $layout-breakpoint-small) {\n    .hide-extra-small {\n        display: none;\n    }\n    .selector1{\n        color:red;\n        .selector1-1{\n            color:green;\n        }\n    }\n}\n\n.class-a{\n    width: 1px;\n}"
